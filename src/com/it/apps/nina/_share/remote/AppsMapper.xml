<?xml version="1.0" encoding="UTF-8"?>  
 <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"   
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">  
<!-- SqlSessionFactory.getMapper()根据namespace，生成接口的实现类，并代理 -->
<mapper namespace="com.it.apps.nina._share.remote.AppsMapper"> 
  	
    <select id="getApps" resultType="com.it.apps.nina._share.remote.AppDto" >
    	select * from apps order by app_id;
    </select>

</mapper>  


